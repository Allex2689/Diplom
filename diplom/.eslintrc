{
  "extends": "react-app",
  "root": true,
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "env": {
    "node": true
  },
  "rules": {
    "no-console": "warn",
    "object-curly-spacing": ["error", "always"],
    "react-hooks/exhaustive-deps": "off"
  }
}

